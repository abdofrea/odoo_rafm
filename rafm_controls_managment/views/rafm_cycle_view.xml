<odoo>
    <record id="view_rafm_cycle_form" model="ir.ui.view">
        <field name="name">rafm.controls.cycle.form</field>
        <field name="model">rafm.controls.cycle</field>
        <field name="arch" type="xml">
            <form string="RAFM Audit Generation Cycle">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_rafm_cycle_tree" model="ir.ui.view">
        <field name="name">rafm.controls.cycle.tree</field>
        <field name="model">rafm.controls.cycle</field>
        <field name="arch" type="xml">
            <tree string="RAFM Audit Cycles">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_rafm_cycle" model="ir.actions.act_window">
        <field name="name">RAFM Audit Cycles</field>
        <field name="res_model">rafm.controls.cycle</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
